is.acyclic <- function(U){
flag <- 1
while (sum(U)>0){
if (min(colSums(U))>0){
flag <- 0
break
}
idx <- which(colSums(U)!=0)
U <- U[idx,idx,drop=FALSE]
}
return(flag)
}
